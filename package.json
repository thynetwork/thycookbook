{
  "name": "thycookbook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run db:deploy && next build --turbopack",
    "build:local": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "db:migrate:prod": "prisma migrate deploy --schema=src/prisma/schema.prisma",
    "db:deploy": "npm run db:generate && npm run db:migrate:prod && npm run db:seed",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:seed:force": "FORCE_SEED=true tsx src/prisma/seed.ts",
    "db:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=src/prisma/schema.prisma",
    "postinstall": "npm run db:generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "next": "15.5.6",
    "next-auth": "^4.24.11",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.23",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
